{
    "name": "background-phi-colors",
    "displayName": "Background Phi Colors",
    "description": "This extension colors the background in various ways.",
    "icon": "images/phihex.128.png",
    "version": "0.3.0",
    "preview": true,
    "publisher": "wraith13",
    "license": "SEE LICENSE IN LICENSE_1_0.txt",
    "engines": {
        "vscode": "^1.30.0"
    },
    "bugs": {
        "url": "https://github.com/wraith13/background-phi-colors-vscode/issues"
    },
    "homepage": "https://github.com/wraith13/background-phi-colors-vscode",
    "repository": {
        "type": "git",
        "url": "https://github.com/wraith13/background-phi-colors-vscode.git"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "background",
        "phi",
        "color"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "background-phi-colors.overTheLimig",
                "title": "Over the Limit",
                "category": "Background Phi Colors"
            },
            {
                "command": "background-phi-colors.pause",
                "title": "Pause",
                "category": "Background Phi Colors"
            },
            {
                "command": "background-phi-colors.pauseAll",
                "title": "Pause All",
                "category": "Background Phi Colors"
            },
            {
                "command": "background-phi-colors.startProfile",
                "title": "Start Profile",
                "category": "Background Phi Colors"
            },
            {
                "command": "background-phi-colors.stopProfile",
                "title": "Stop Profile",
                "category": "Background Phi Colors"
            }
        ],
        "configuration": [
            {
                "title": "background-phi-colors",
                "properties": {
                    "background-phi-colors.enabled": {
                        "type": "boolean",
                        "default": true,
                        "overridable": true
                    },
                    "background-phi-colors.enabledPanels": {
                        "type": "boolean",
                        "default": false,
                        "overridable": true
                    },
                    "background-phi-colors.fileSizeLimit": {
                        "type": "number",
                        "default": 102400,
                        "minimum": 10240,
                        "maximum": 10485760,
                        "overridable": true
                    },
                    "background-phi-colors.basicDelay": {
                        "type": "number",
                        "default": 10,
                        "minimum": 1,
                        "maximum": 1500,
                        "overridable": true
                    },
                    "background-phi-colors.additionalDelay": {
                        "type": "number",
                        "default": 200,
                        "minimum": 50,
                        "maximum": 1500,
                        "overridable": true
                    },
                    "background-phi-colors.baseColor": {
                        "type": "string",
                        "default": "#CC666666",
                        "overridable": true
                    },
                    "background-phi-colors.indentMode": {
                        "type": "string",
                        "default": "full",
                        "enum": ["none", "light", "smart", "full"],
                        "overridable": true
                    },
                    "background-phi-colors.tokenMode": {
                        "type": "string",
                        "default": "smart",
                        "enum": ["none", "light", "smart", "full"],
                        "overridable": true
                    },
                    "background-phi-colors.indentErrorEnabled": {
                        "type": "boolean",
                        "default": true,
                        "overridable": true
                    },
                    "background-phi-colors.traillingSpacesErrorEnabled": {
                        "type": "boolean",
                        "default": true,
                        "overridable": true
                    },
                    "background-phi-colors.bodySpacesEnabled": {
                        "type": "boolean",
                        "default": true,
                        "overridable": true
                    },
                    "background-phi-colors.traillingSpacesEnabled": {
                        "type": "boolean",
                        "default": true,
                        "overridable": true
                    },
                    "background-phi-colors.symbolEnabled": {
                        "type": "boolean",
                        "default": false,
                        "overridable": true
                    },
                    "background-phi-colors.showIndentErrorInOverviewRulerLane": {
                        "type": "boolean",
                        "default": true,
                        "overridable": true
                    },
                    "background-phi-colors.showActiveTokenInOverviewRulerLane": {
                        "type": "boolean",
                        "default": true,
                        "overridable": true
                    },
                    "background-phi-colors.showTraillingSpacesErrorInOverviewRulerLane": {
                        "type": "boolean",
                        "default": true,
                        "overridable": true
                    },
                    "background-phi-colors.spacesAlpha": {
                        "type": "number",
                        "default": 17,
                        "minimum": 0,
                        "maximum": 255,
                        "overridable": true
                    },
                    "background-phi-colors.spacesActiveAlpha": {
                        "type": "number",
                        "default": 51,
                        "minimum": 0,
                        "maximum": 255,
                        "overridable": true
                    },
                    "background-phi-colors.spacesErrorAlpha": {
                        "type": "number",
                        "default": 136,
                        "minimum": 0,
                        "maximum": 255,
                        "overridable": true
                    },
                    "background-phi-colors.symbolAlpha": {
                        "type": "number",
                        "default": 68,
                        "minimum": 0,
                        "maximum": 255,
                        "overridable": true
                    },
                    "background-phi-colors.tokenAlpha": {
                        "type": "number",
                        "default": 51,
                        "minimum": 0,
                        "maximum": 255,
                        "overridable": true
                    },
                    "background-phi-colors.tokenActiveAlpha": {
                        "type": "number",
                        "default": 102,
                        "minimum": 0,
                        "maximum": 255,
                        "overridable": true
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.42",
        "tslint": "^5.13.1",
        "typescript": "^3.3.3333",
        "vscode": "^1.1.30"
    },
    "dependencies": {
        "phi-colors": "^1.0.0"
    }
}
